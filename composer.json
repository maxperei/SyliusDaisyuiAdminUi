{
    "name": "sylius/daisyui-admin-ui",
    "type": "library",
    "keywords": [
        "symfony-ux"
    ],
    "require": {
        "sylius/admin-ui": "^0.10",
        "sylius/grid-bundle": "^1.14",
        "sylius/resource-bundle": "^1.13",
        "sylius/twig-extra": "^0.10",
        "sylius/twig-hooks": "^0.10",
        "symfony/http-kernel": "^6.4 || ^7.4 || ^8.0",
        "symfony/http-client": "^6.4 || ^7.4 || ^8.0",
        "symfony/security-bundle": "^6.4 || ^7.4 || ^8.0",
        "symfony/ux-icons": "^2.32",
        "tales-from-a-dev/twig-tailwind-extra": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Sylius\\DaisyuiAdminUi\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/runtime": true
        },
        "sort-packages": true
    },
    "repositories": [
        {
            "type": "path",
            "url": "../**"
        }
    ],
    "extra": {
        "symfony": {
            "require": "7.4.*"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
